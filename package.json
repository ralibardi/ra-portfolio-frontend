{
  "name": "ra-portfolio",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "pnpm run build:dev && vite",
    "build": "tsc && vite build",
    "build:dev": "tsc && vite build --mode development",
    "watch": "vite build --watch",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "preview": "vite preview",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "jest -u",
    "test:coverage": "jest -u --coverage",
    "test:watch": "jest -u --watch",
    "update:husky": "pnpm update husky lint-staged @biomejs/biome",
    "clean": "rimraf dist",
    "clean:all": "rimraf dist node_modules package-lock.json",
    "sass:migrate": "find . -name '*.scss' -not -path '*node_modules*' -exec sass-migrator module --migrate-deps {} \\;"
  },
  "dependencies": {
    "history": "5.3.0",
